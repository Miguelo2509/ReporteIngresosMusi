            ON "EINA"."MATNR" = "MARA"."MATNR"
        ) 
INNER JOIN
    "LFA1" "LFA1" 
        ON "EINA"."LIFNR" = "LFA1"."LIFNR" 
WHERE
    (
        "EINA"."RELIF" = 'X'
    ) 
    AND (
        "MARA"."MTART" = 'ZPRO' 
        OR "MARA"."MTART" = 'ZBLA' 
        OR "MARA"."MTART" = 'ZPVV'
    )
 ) END_QUERY 

